{
    "name": "softifyvn/stf-orm",
    "license": "BSD-3-Clause",
    "keywords": [
        "softify",
        "stf",
        "orm"
    ],
    "homepage": "https://github.com/softifyvn/stf-orm",
    "autoload": {
        "psr-4": {
            "Stf\\Orm\\": "src/"
        }
